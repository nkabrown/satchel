<h1>Unlock your Satchel and start saving your keepsakes</h1>

<%= form_tag("/sessions", method: :post) do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %><br>
  <%= label_tag :password %>
  <%= password_field_tag :password %><br>
  <%= label_tag :password_confirmation %>
  <%= password_field_tag :password_confirmation %><br>
  <%= submit_tag "Unlock" %>
<% end %>
